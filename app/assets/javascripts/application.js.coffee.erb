$ ->
  $('.js-subscribe-story').change ->
    subscribe_story_link = $('.js-subscribe-story').val()
    $.ajax
      url: subscribe_story_link
      type: 'POST'
    return

$ ->
  $('.js-subscribe-edm').change ->
    subscribe_edm_link = $('.js-subscribe-edm').val()
    $.ajax
      url: subscribe_edm_link
      type: 'POST'
    return
