{
  "name": "jrf-sunny",
  "version": "1.0.0",
  "description": "A website for jrf-sunny",
  "main": "client/index.coffee",
  "repository": {
    "type": "git",
    "url": "git@github.com:5fpro/jrf-sunny.git"
  },
  "author": "Jude Wang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/5fpro/jrf-sunny/issues"
  },
  "homepage": "https://github.com/5fpro/jrf-sunny#readme",
  "publicPath": "/assets/",
  "scripts": {
    "start": "redis-server & bundle exec rails s & bundle exec guard -i & npm run dev-server",
    "rails": "redis-server & bundle exec rails s & bundle exec guard -i",
    "console": "redis-server & bundle exec rails c",
    "update": "gulp update",
    "check": "rubocop --auto-correct",
    "rebase": "bundle install && npm run migrate",
    "migrate": "redis-server & bundle exec rake db:migrate",
    "test": "redis-server & bundle exec rake spec",
    "dev-server": "webpack-dev-server --inline --hot --config webpack-dev.config.coffee",
    "build": "webpack --config webpack.config.coffee",
    "postbuild": "gulp resolve-url",
    "clean": "rake tmp:clear && rake tmp:sessions:clear"
  },
  "dependencies": {
    "animate.css": "^3.5.1",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "chosen-js": "^1.6.2",
    "eslint-config-rallycoding": "^3.1.0",
    "js-cookie": "^2.1.3",
    "lazysizes": "^2.0.0",
    "magnific-popup": "^1.1.0",
    "modernizr": "^3.3.1",
    "modularscale-sass": "^2.1.1",
    "node.normalize.scss": "^3.0.3",
    "picturefill": "3.x",
    "sass-toolkit": "^2.10.0",
    "scut": "^1.4.0",
    "slick-carousel": "^1.6.0",
    "susy": "^2.2.12",
    "waypoints": "^4.0.0",
    "webui-popover": "^1.2.13"
  },
  "devDependencies": {
    "animate-css-webpack": "^3.5.6",
    "autoprefixer": "^6.4.0",
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.10.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "git-branch": "^0.3.0",
    "gulp": "^3.9.1",
    "gulp-error-notifier": "^1.1.0",
    "gulp-replace": "^0.5.4",
    "gulp-shell": "^0.5.2",
    "img-loader": "^1.3.1",
    "modernizr-loader": "0.0.5",
    "node-sass": "^3.8.0",
    "postcss-assets": "^4.1.0",
    "postcss-calc": "^5.3.0",
    "postcss-clearfix": "^1.0.0",
    "postcss-loader": "^0.10.0",
    "postcss-short": "^1.4.0",
    "postcss-strip-units": "^1.2.0",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "^3.2.3",
    "sass-resources-loader": "^1.1.0",
    "script-loader": "^0.7.0",
    "style-loader": "^0.13.1",
    "svg-sprite-loader": "0.0.26",
    "svgo": "^0.6.6",
    "svgo-loader": "^1.1.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.1",
    "yaml-loader": "^0.4.0"
  }
}
